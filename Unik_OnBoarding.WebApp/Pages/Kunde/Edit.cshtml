<div class="row">
	<div class="col-md-4">
		<form method="post">
			<input type="hidden" asp-for="Urt.RowVersion" class="form-control" />
			<input type="hidden" asp-for="Urt.Kid" class="form-control" />
			<div class="form-group">
				<label asp-for="Urt.Fornavn" class="control-label"></label>
				<input asp-for="Urt.Fornavn" class="form-control" required/>

			<div class="form-group">
				<label asp-for="Urt.Email" class="control-label"></label>
				<input asp-for="Urt.Email" class="form-control" />
				<span asp-validation-for="Urt.Email" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="Urt.Telefon" class="control-label"></label>
				<input asp-for="Urt.Telefon" class="form-control" />
				<span asp-validation-for="Urt.Telefon" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="Urt.Adresse" class="control-label"></label>
				<input asp-for="Urt.Adresse" class="form-control" />
				<span asp-validation-for="Urt.Adresse" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Update Kunde" class="btn btn-primary form-control" />
			</div>
		</form>
	</div>
</div>

<div>
	<a asp-page="Index">Back to List</a>
</div>


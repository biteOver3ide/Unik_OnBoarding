@page
@model Unik_OnBoarding.WebApp.Pages.Projekt.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit Projekt</h2>
<hr />
<div class="row">
	<div class="col-md-4">
		<form method="post">
			<input type="hidden" asp-for="Urt.RowVersion" class="form-control" />
			<input type="hidden" asp-for="Urt.ProjektId" class="form-control" />
			<div class="form-group">
				<label asp-for="Urt.ProjektTitle" class="control-label"></label>
				<input asp-for="Urt.ProjektTitle" class="form-control" />
				<span asp-validation-for="Urt.ProjektTitle" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Update Projekt" class="btn btn-primary form-control" />
			</div>
		</form>
	</div>
</div>

<div>
	<a asp-page="Index">Back to List</a>
</div>

@section Scripts {
	@{
		await Html.RenderPartialAsync("_ValidationScriptsPartial");
	}
	}